<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".systermKnowledge.sharePreferences.SharePreferencesActivity">

    <Button
        android:id="@+id/btn_sp_write"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:text="startSave"
        android:onClick="startSave"/>

    <Button
        android:id="@+id/btn_sp_read"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/btn_sp_write"
        android:text="startRead"
        android:onClick="startRead"/>
</androidx.constraintlayout.widget.ConstraintLayout>